{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "protocol": "inspector",
      "cwd": "${workspaceRoot}/test/dir/workspace/",
      "args": [
        "${workspaceRoot}/test/configLoader.js"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "smartStep": true,
      "name": "run test",
      "request": "launch",
      "type": "node"
    },
    {
      "name": "ts-node",
      "type": "node",
      "request": "launch",
      "args": [
        "${relativeFile}",
        "--loader ts-node/esm.mjs"
      ],
      "runtimeArgs": [],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
